#!/bin/bash -l

%includeonce <ecflow_bash.h>
%includeonce <ens.h>
%includeonce <geff.h>

fctype=%FCTYPE%
tarname=ECMWF_EFFIS_${ens_ymd}_1200_${fctype}

yyyy=${ens_ymd:0:4}
mm=${ens_ymd:4:2}
export ECUMASK=022
archdir=ec:/$USER/geff/$expver/rt/$yyyy/$mm/
emkdir -p $archdir
ecp -o $ens_tar_dir/$tarname.tar $archdir/
